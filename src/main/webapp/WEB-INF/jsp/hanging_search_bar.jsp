<!-- 悬浮搜索框 下滑会消失 -->
<div class="search-area">
    <form id="search_form" action="/discovery/materials/"  method="get" novalidate>
        <div contentEditable="true" class="input-holder">
            <input id="searchkey" type="search" name="q" 
            contentEditable="true" class="search-input"
                 role="combobox"
                minlength="3" aria-haspopup="false" aria-expanded="false"
                autocomplete="off">
        </div>
    </form>
</div>